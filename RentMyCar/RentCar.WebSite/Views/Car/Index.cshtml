@model IEnumerable<DataEntity.Car>

@{
    ViewBag.Title = "Index";
}

<h2>Araba Bilgileri</h2>

<p>
    

    <a href="/Car/Create" class="btn btn-success"> Yeni Araba</a>

    <a href="/Admin/Index" class="btn btn-danger"> Admin Panele Dön</a>


</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ImageUrl)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.ArabaAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Yıl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IndirimOrani)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KasaTipi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YakitTipi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VitesTipi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Depozito)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GunlukUcret)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.SürücüYas)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Marka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Locations)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ImageUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ArabaAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Yıl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IndirimOrani)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KasaTipi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YakitTipi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VitesTipi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>


        <td>
            @Html.DisplayFor(modelItem => item.Depozito)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GunlukUcret)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.SürücüYas)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Marka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Locations)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Sil", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
