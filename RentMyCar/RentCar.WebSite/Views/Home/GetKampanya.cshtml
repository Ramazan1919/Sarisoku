
@using DataEntity
@model IEnumerable<Kampanya>
@{
    ViewBag.Title = "GetKampanya";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="main">

    <section id="page" class="campains">
        <div class="container-fluid">
            <div class="header">
                <div class="row">
                    <div class="col-sm-6">
                        <h1>KAMPANYALAR</h1>
                        <p>Tüm güncel kampanya ve sağladığımız avantajları,<br> buradan öğrenebilirsiniz.</p>
                    </div>

                </div>
            </div>
            <article>
                <div class="row">
                    @foreach (var item in Model)
                    {
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <figure>
                                <a href="/Home/KampanyaDetay/@item.Id">
                                    <img class="" src="~/Content/Admin/Content/Photos/Kampanya/@item.kampanyaUrl" alt="...">
                                    <figcaption>
                                        <a href="/Home/KampanyaDetay/@item.Id" class="commonBtn">Detaylı Bilgi</a>
                                    </figcaption>
                                </a>
                            </figure>
                        </div>
                    }

                </div>

            </article>
        </div>
    </section>


</div>


